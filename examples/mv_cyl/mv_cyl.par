userSections = CASEDATA

[GENERAL]
#verbose=true
polynomialOrder = 7
stopAt = numSteps
numSteps = 100
dt = 1e-3
timeStepper = tombo3
checkpointInterval = 50

scalars = TEMPERATURE

[GEOM]
boundaryTypeMap = udfDirichlet, zeroDirichlet, zeroDirichletN/zeroNeumann  
residualTol = 1e-6
solver = pcg+block #user
initialGuess = projection+nVector=1 

[FLUID PRESSURE]
residualTol = 1e-8
initialGuess = previous

[FLUID VELOCITY]
boundaryTypeMap = udfDirichlet, zeroDirichlet, zeroDirichletN/zeroNeumann  
residualTol = 1e-9

[SCALAR TEMPERATURE]
boundaryTypeMap = zeroNeumann, zeroNeumann, zeroNeumann
residualTol = 1e-12

[CASEDATA]
XLEN = 1.0
YLEN = 1.5
ZLEN = 0.1

P_GAMMA = 1.4
P_OMEGA = 3.141592653589793
P_AMP = 1.5707963267948966
P_ROT = 0.0

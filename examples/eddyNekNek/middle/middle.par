userSections = CASEDATA

[GENERAL]
polynomialOrder = 7
#startFrom = "restart.fld"
numSteps = 100
dt = 1e-04
timeStepper = tombo2
checkpointInterval = 20

udf = "../eddy.udf"
usr = "../inside/inside.usr"

scalars = PS1

[NEKNEK]
boundaryEXTOrder = 1

[FLUID PRESSURE]
residualTol = 1e-08

[FLUID VELOCITY]

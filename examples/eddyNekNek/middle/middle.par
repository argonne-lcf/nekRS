transportCoeff = 1.0
diffusionCoeff = 0.05
boundaryTypeMap = int

[CASEDATA]
P_U0 = 5.0
P_V0 = 1.5
P_OMEGA = 2 # multiplied by pi
P_SCALE = 1.5 # multiplied by pi

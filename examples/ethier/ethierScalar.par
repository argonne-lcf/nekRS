userSections = CASEDATA

[GENERAL]
#verbose = true 
polynomialOrder = 9
#startFrom = "restart.fld"
stopAt = numSteps
numSteps = 100 
dt = 2e-03
timeStepper = tombo3
checkpointControl = simulationTime 
checkpointInterval = 0.1

usr = "ethier.usr"
oudf = "ethier.oudf"
udf = "ethier.udf"

scalars = VELX_DIR, VELX_NEU

[MESH]
file = "ethier.re2"

[FLUID VELOCITY]
solver = none
rho = 1.0
viscosity = 1/100

[SCALAR VELX_DIR]
solver = none
boundaryTypeMap = udfDirichlet 
residualTol = 1e-12
transportCoeff = 1.0
diffusionCoeff = 1/100

[SCALAR VELX_NEU]
boundaryTypeMap = udfNeumann
residualTol = 1e-12
transportCoeff = 1.0
diffusionCoeff = 1/100

[CASEDATA]
P_U0 = 0.5
P_V0 = 0.1
P_W0 = 0.2
P_A0 = 0.025
P_D0 = 0.5
P_OMEGA = 15.0
P_AMP = 1.5


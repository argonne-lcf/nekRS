[GENERAL] 
#startFrom = "restart.fld"
polynomialOrder = 7
numSteps = 1000 
dt = 2e-02
timeStepper = tombo2
checkpointInterval = 100

scalars = foo, temperature

[FLUID PRESSURE]
residualTol = 1e-06

[FLUID VELOCITY]
boundaryTypeMap = udfDirichlet, zeroNeumann, zeroDirichlet 
residualTol = 1e-08

[SCALAR FOO]
solver = none

[SCALAR TEMPERATURE]
boundaryTypeMap = udfDirichlet, zeroNeumann, zeroNeumann 
residualTol = 1e-08
mesh = fluid + solid

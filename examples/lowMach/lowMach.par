userSections = CASEDATA

[GENERAL] 
polynomialOrder = 7 
stopAt = numSteps
numSteps = 300
dt = 1e-03
timeStepper = tombo2
checkpointInterval = 100

scalars = TEMPERATURE

[FLUID PRESSURE]
residualTol = 1e-08
initialGuess = previous

[FLUID VELOCITY]
boundaryTypeMap = udfDirichlet 
residualTol = 1e-12

[SCALAR TEMPERATURE] 
boundaryTypeMap = udfDirichlet 
residualTol = 1e-12

[CASEDATA]
P_DELTA  = 0.2
P_GAMMA  = 1.4
NUMBER_ELEMENTS_X = 8
NUMBER_ELEMENTS_Y = 8
NUMBER_ELEMENTS_Z = 1

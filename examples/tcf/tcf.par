[GENERAL]
polynomialOrder = 7
cubaturePolynomialOrder = 9

#startFrom = "r.fld+time=0"
stopAt = endTime
endTime = 200 

timeStepper = tombo2
dt = targetCFL=2.4 + max=1e-2
advectionSubCyclingSteps = 1

checkpointControl = simulationTime 
checkpointInterval = 25 

regularization = hpfrt + nModes=1 + scalingCoeff=5

[FLUID PRESSURE]
residualTol = 1e-04

[FLUID VELOCITY]
boundaryTypeMap = udfDirichlet, zeroDirichlet, none, none
viscosity = 1/2500.0 # Re = U0 * d / nu
rho = 1.0
residualTol = 1e-06

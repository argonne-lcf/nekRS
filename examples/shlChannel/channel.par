userSections = CASEDATA

[GENERAL]
polynomialOrder = 7
stopAt = numSteps
numSteps = 100
dt = 1e-03
timeStepper = tombo2
advectionSubCyclingSteps = 2

checkpointControl = steps
checkpointInterval = 100

[FLUID PRESSURE]
residualTol = 1e-08

[FLUID VELOCITY]
boundaryTypeMap = zeroDirichletN/udfNeumann, zeroDirichletN/zeroNeumann 
residualTol = 1e-12
rho = 1.0
viscosity = 1/100.0

[CASEDATA]
P_ROT = 0.5
